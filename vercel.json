{
    "version": 2,
    "builds": [
      {
        "src": "client/dist/**",
        "use": "@vercel/static"
      },
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/client/dist/(.*)",
        "dest": "/client/dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/client/dist/index.html"
      }
    ]
  }
  